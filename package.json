{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "lint:es": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build-netlify": "next build && cp -r .next _next && mv _next .next/",
    "export": "yarn build && next export -o _static",
    "format:check": "prettier --check . --config .prettierrc",
    "format:fix": "prettier --write . --config .prettierrc",
    "check:lint": "eslint --ext ts --ext tsx --ext js --max-warnings=0",
    "check:types": "tsc --pretty --noEmit",
    "check:health": "npm install && npm run format:fix && npm run check:lint && npm run check:types && npm run build",
    "commit:check": "npm run check:lint && npm run check:types && npm run format:check",
    "start:local": "env-cmd -f .env.local npm run dev",
    "start:dev": "env-cmd -f .env.development npm run dev",
    "start:stage": "env-cmd -f .env.stage npm run dev",
    "start:prod": "env-cmd -f .env.production npm run dev",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:stage": "env-cmd -f .env.stage npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "prepare": "husky"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next": "14.1.4",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "typescript": "^5"
  }
}
